print("1")
number1 = 1
while number1 <= 21:
    print(number1)
    number1 += 2
print("2")
number2 = 2
while number2 <= 17:
    print(number2)
    number2 += 3
print("3")
number3 = 100
while number3 >= -90:
    print(number3)
    number3 -= 10
print("4")
number4 = -30
while number4 <= 30:
    print(number4)
    number4 += 10
print("5")
number = 15
while number5 <= 55:
    print(number5)
    number5 += 8
